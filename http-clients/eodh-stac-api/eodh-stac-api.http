### Search S2 ARD TEST
POST https://test.eodatahub.org.uk/api/catalogue/stac/catalogs/public/ceda-stac-catalogue/search
Accept: application/json
Content-Type: application/json
Authorization: Bearer {{token}}

{
   "limit":1,
   "sortby":[
      {
         "field":"properties.datetime",
         "direction":"desc"
      }
   ],
   "filter-lang":"cql-json",
   "filter":{
      "op":"and",
      "args":[
         {
            "op":"between",
            "args":[
               {
                  "property":"properties.datetime"
               },
               "2020-01-18T00:00:00.000Z",
               "2025-02-18T23:59:59.999Z"
            ]
         },
         {
            "op":"and",
            "args":[
               {
                  "op":"=",
                  "args":[
                     {
                        "property":"collection"
                     },
                     "sentinel2_ard"
                  ]
               },
               {
                  "op":"<=",
                  "args":[
                     {
                        "property":"properties.eo:cloud_cover"
                     },
                     70
                  ]
               }
            ]
         }
      ]
   },
   "fields":{
      "include":[
         "properties.eo:cloud_cover",
         "properties.grid:code"
      ],
      "exclude":[

      ]
   },
   "intersects":{
      "type":"Polygon",
      "coordinates":[
         [
            [
